projection;
strict ( 2 );
use draft;

define behavior for ZC_SETUP_GR_HEADER //alias <alias_name>
{
  use create;
  use update;
  use delete;

  use action Edit;
  use action Activate;
  use action Discard;
  use action Resume;
  use action Prepare;
  use action getItem;
  use action getpdf;

  field ( mandatory : create ) Plant, Manufacturingorder, Material, Batch;

  use association _Item { create; with draft; }

}

define behavior for ZC_SETUP_GR_ITEM //alias <alias_name>
{
  use update;
  use delete;

  use association _HDR { with draft; }
}
